{
    "name": "vue-antd-ui",
    "version": "0.6.5",
    "title": "Ant Design Vue",
    "description": "An enterprise-class UI design language and Vue-based implementation",
    "keywords": [
        "ant",
        "design",
        "antd",
        "vue",
        "vueComponent",
        "component",
        "components",
        "ui",
        "framework",
        "frontend"
